@model IEnumerable<CmsShopingCart.Models.ViewModels.PagesVM>

@{
    string pageTitle = (string)TempData["PageTitle"];
}

@foreach (var item in Model)
{
    if (pageTitle == item.Title)
    {
        <li class="active" style="color: lightpink"><a href="/@item.Slug" style="color: deeppink; background-color:darkslategray">@item.Title</a></li>
    }
    else
    {
        <li><a href="/@item.Slug" style="color: lightpink;" >@item.Title</a></li>
    }
}


